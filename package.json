{
  "name": "avetis",
  "version": "1.0.0",
  "description": "Charity program",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix avetis",
    "client:install": "npm install --prefix avetis",
    "client:build": "npm run build --prefix avetis",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "Give a gift get a smile",
    "Christmas"
  ],
  "author": "Gor Baghramyan <gorbaghramyan@outlook.com>, Arman Sargsyan",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.9.0",
    "firebase": "^8.2.1",
    "framer-motion": "^3.1.1",
    "mongoose": "^5.11.8"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6"
  }
}
